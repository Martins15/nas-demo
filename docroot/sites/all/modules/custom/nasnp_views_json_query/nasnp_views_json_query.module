/**
 * @file
 * NAS Native Plants Views JSON Query module.
 */

/**
 * Implements hook_views_api().
 */
function nasnp_views_json_query_views_api() {
  return array(
    'api' => 3,
    // 'path' => drupal_get_path('module', 'nasnp_views_json_query'),
  );
}

/**
 * Helper function to store unfiltered results.
 */
function nasnp_view_json_query_results_unfiltered($results = array()) {
  $stored = &drupal_static(__FUNCTION__);
  if ($results) {
    $stored = $results;
  }

  return $stored;
}
